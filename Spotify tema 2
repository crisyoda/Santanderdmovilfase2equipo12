 main(args: Array<String>) {
    var lista1: List<String> = listOf("Electronicamix-varios", "Como la flor-Selena", "One more,dj", "Ameno-Era", "Atrévete","mingo")
    println("Reproducir la Playlist completá")
    println(lista1)
    println("Reproduciendo la primer canción la Playlist")
    println(lista1[0])
    println("Play el primer elemento de la Playlist ")
    println(lista1.first())
    println("Aleatorio  cancion de la Playlist ")
    println(lista1.last())
    println("Ultima canción de la Playlist ")
    println(lista1[lista1.size-1])
    println("Reproducir cantidad de canciones de la playlist")
    println(lista1.size)
    println("Recorrer la lista completa de reproducción")
    for(elemento in lista1)
        print("$elemento ")
    println()
    println("Mostrar cancion la playlisten  y su posición ")
    for(posicion in lista1.indices)
        print("[$posicion]${lista1[posicion]} ")
